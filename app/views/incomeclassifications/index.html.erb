<h1>Listing income classifications</h1>
<div class="paginate"><%= will_paginate %></div><br />
<table>
  <tr>
    <th>Description</th>
  </tr>

<% @incomeclassifications.each do |incomeclassification| %>
  <tr>
    <td><%=h incomeclassification.description %></td>
    <td><%= link_to 'Show', incomeclassification %></td>
    <td><%= link_to 'Edit', edit_incomeclassification_path(incomeclassification) %></td>
    <td><%= link_to 'Destroy', incomeclassification, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>
<div class="paginate"><%= will_paginate %></div><br />

<%= link_to 'New incomeclassification', new_incomeclassification_path %>