<!-- Search results page -->
<h1>Listing people - search result</h1> 

<div class="paginate">
    <%= will_paginate %>
</div><br />

<table>
  <tr>
    <th>Party ID</th>
    <th>Current last name</th>
    <th>Current first name</th>
    <th>Birth date</th>
    <th>Income classification</th>
    <th>Occupation classification</th>
    <% if admin_user? %>
        <th>User account name</th>
    <% end %>
  </tr>
  <%= render @people %>     <!-- calls the partial _person.html.erb for each Person object collected in the @people array-->
</table>

<%= link_to 'New person', new_person_path %>  |
<%= link_to 'New search', search_form_path %>  |
<%= link_to 'Home', root_path %>